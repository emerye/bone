;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	beaglebone.emerye.net.  root.emerye.net. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	beaglebone.
@	IN	A       192.168.1.10 
@	IN	A       192.168.1.20 
10      IN	PTR     beaglebone.emerye.net	
20      IN	PTR     dell960.emerye.net	
