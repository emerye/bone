;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	bone.emerye.net.  root.emerye.net. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	bone.
@	IN	A       192.168.1.10 
@       IN      A       192.168.1.11
@	IN	A       192.168.1.20 
10      IN	PTR     bone.emerye.net	
11      IN      PTR     bone1.emerye.net
20      IN	PTR     dell960.emerye.net	
