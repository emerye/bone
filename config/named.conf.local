// DNS for emerye.net
zone   "emerye.net"   {
        type master;
        file    "/etc/bind/for.emerye.net";
 };

zone   "1.168.192.in-addr.arpa"        {
       type master;
       file    "/etc/bind/rev.emerye.net";
 };

